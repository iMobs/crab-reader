{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "crab-reader",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "tech.imobs.dev",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEIxRkU2Q0QwMEIzQzkwRUUKUldUdWtEd0wwR3orc2ZHRGR4UnlaNWlYTk9xMndoWklhQ01pVXBQUk5Tank0RjE1dU8zYVI1cmgK"
    },
    "windows": [
      {
        "fullscreen": false,
        "maximized": true,
        "resizable": true,
        "title": "Crab Reader",
        "width": 800,
        "height": 600
      }
    ]
  }
}
